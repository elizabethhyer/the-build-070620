<% @errors && @errors.each do |m| %>
    <li><%= m %></li>
<% end %>

<%= form_for @project do |f| %>
    <%= f.label "Name: " %>
    <%= f.text_field :name %>
    <%= f.label "Technique Used: " %>
    <%= f.text_field :technique_used %>
    <%= f.label "Material Used: " %>
    <%= f.text_field :material_used %>
    <%= f.label "Discipline: " %>
    <%= f.collection_select :discipline_id, Discipline.all, :id, :kind %>
    <%= f.submit %>
<% end %>